{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python"
        },
        {
            "src": "static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/result",
            "dest": "/api/index.py"
        },
        {
            "src": "/analyze",
            "methods": ["POST"],
            "dest": "/api/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.py"
        }
    ],
    "functions": {
        "api/index.py": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "build": {
        "env": {
            "PYTHON_VERSION": "3.9"
        }
    }
}
