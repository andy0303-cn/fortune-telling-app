{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9",
                "buildCommand": "python -V"
            }
        },
        {
            "src": "static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/result",
            "dest": "/api/index.py"
        },
        {
            "src": "/analyze",
            "methods": ["POST"],
            "dest": "/api/index.py"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.py"
        }
    ],
    "github": {
        "silent": true,
        "autoJobCancelation": true
    }
}
